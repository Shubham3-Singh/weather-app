<div class="commonForm">
  <h1>Sign Up</h1>
  <%= form_with(model: @user, method: :post) do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <br/>
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
    <br/>
    <br/>
    <%= form.submit 'Sign Up' %>
    <%= form.submit 'Login' %>
  <% end %>
</div>